# Vercel ignore file for Laravel
# Only ignore what's absolutely necessary

# Development files
.env.local
.env.development
.env.testing

# Cache and logs
storage/logs/*
storage/framework/cache/*
storage/framework/sessions/*
storage/framework/views/*

# Node modules (will be built by Vercel)
node_modules/

# IDE files
.vscode/
.idea/

# OS files
.DS_Store
Thumbs.db

# Git
.git/

# Don't ignore these important Laravel files:
# !vendor/
# !bootstrap/
# !config/
# !database/
# !public/
# !resources/
# !routes/
# !app/ 