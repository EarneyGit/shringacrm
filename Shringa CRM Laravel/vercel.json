{
  "version": 2,
  "builds": [
    {
      "src": "api/index.php",
      "use": "vercel-php@0.7.0"
    }
  ],
  "routes": [
    {
      "src": "/(css|js|images|build)/(.*)",
      "dest": "/public/$1/$2"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_KEY": "base64:VQuaswEMbnGKGEQWFieSlr8N9TkPpAy22GNeAijIgOg=",
    "APP_URL": "https://shringacrm.vercel.app",
    "SESSION_DRIVER": "file",
    "CACHE_STORE": "file",
    "QUEUE_CONNECTION": "sync",
    "LOG_CHANNEL": "stack",
    "LOG_LEVEL": "error"
  },
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.0"
    }
  }
} 